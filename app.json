{
  "expo": {
    "name": "Presenzo",
    "slug": "presenzo-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.alphadevs.presenzo"
    },
    "android": {
      "package": "com.alphadevs.presenzo", 
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-barcode-scanner",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Presenzo to access your camera."
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Presenzo to use your location."
        }
      ]
    ],
    "experiments": {
      "tsconfigPaths": true
    }
  }
}